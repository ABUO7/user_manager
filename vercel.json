{
    "builds": [{
      "src": "user_manager/user_manager/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11.9"
      }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "user_manager/user_manager/wsgi.py"
      }
    ]
}